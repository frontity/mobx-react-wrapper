{
  "name": "mobx-react-wrapper",
  "version": "0.0.1",
  "author": "luisherranz",
  "repository": {
    "type": "git",
    "url": "https://github.com/frontity/mobx-react-wrapper.git"
  },
  "bugs": {
    "url": "https://github.com/frontity/mobx-react-wrapper/issues"
  },
  "homepage": "https://github.com/frontity/mobx-react-wrapper#readme",
  "main": "dist/mobx-react-wrapper.js",
  "umd:main": "dist/mobx-react-wrapper.umd.js",
  "module": "dist/mobx-react-wrapper.m.js",
  "source": "src/index.js",
  "license": "Apache-2.0",
  "scripts": {
    "example": "parcel ./example/index.html --out-dir exampleDist",
    "build": "microbundle -o dist/ --sourcemap false",
    "dev": "microbundle watch -o dist/ --sourcemap false --compress false",
    "test": "jest --watch --config=jest.config.js",
    "test:coverage": "jest --coverage --config=jest.config.js",
    "test:ci": "jest --config=jest.config.js",
    "report-coverage": "cat ./coverage/lcov.info | coveralls"
  },
  "keywords": [
    "mobx",
    "inject",
    "wrapper",
    "hooks",
    "react",
    "utils"
  ],
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@types/jest": "^23.3.10",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "coveralls": "^3.0.2",
    "eslint": "^5.7.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-react-hooks": "next",
    "jest": "^23.6.0",
    "jest-dom": "^3.0.0",
    "microbundle": "^0.6.0",
    "parcel-bundler": "^1.10.3",
    "react": "16.8.0-alpha.0",
    "react-dom": "16.8.0-alpha.0",
    "mobx": "5.0.0",
    "mobx-state-tree": "3.0.0",
    "react-testing-library": "^5.4.2",
    "regenerator-runtime": "^0.13.1"
  },
  "peerDependencies": {
    "mobx": "^4.0.0 || ^5.0.0",
    "react": "16.7.0-alpha.2 || 16.8.0-alpha.0"
  },
  "dependencies": {
    "mobx-react-lite": "^0.3.5"
  }
}
